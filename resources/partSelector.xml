<?xml version="1.0" encoding="UTF-8"?>

  <!-- part selector configuration -->
  <PartSelector>
    <name>PartSelector</name>
<!--    verbose : 0 -> no debug
		  1 -> show debug output 
	distanceMeasure:
		  0 -> dot product of normals
		  1 -> dot product of normals and Euclidean distance
		  2 -> dot product of normals and Mahalanobis distance
    -->
    <parameters verbose="1" distanceMetric="2" clustersSecondLayer="20" clustersThirdLayer="15" clustersFifthLayer="9" clustersSixthLayer="5" maxIter="10"></parameters>
  </PartSelector>