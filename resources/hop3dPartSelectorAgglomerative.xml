<?xml version="1.0" encoding="UTF-8"?>

  <!-- part selector configuration -->
  <PartSelector>
    <name>PartSelector</name>
<!--    verbose : 0 -> no debug
		  1 -> show debug output 
	distanceMeasure:
		  0 -> dot product of normals
		  1 -> dot product of normals and Euclidean distance
		  2 -> dot product of normals and Mahalanobis distance
		  3 -> view invariant metric
    -->
    <parameters verbose="0" distanceMetric="3" layersNo="7" maxIter="10"></parameters>
    <layer1 maxDist="0.0" maxClusterDist="0.0"></layer1> <!--constant size-->
    <layer2 maxDist="0.45" maxClusterDist="0.46"></layer2>
    <layer3 maxDist="0.50" maxClusterDist="0.51"></layer3>
    <layer4 maxDist="4.0004" maxClusterDist="4.00041"></layer4>
    <layer5 maxDist="0.02" maxClusterDist="0.021"></layer5>
    <layer6 maxDist="0.09" maxClusterDist="0.091"></layer6>
    <layer7 maxDist="0.7" maxClusterDist="0.7"></layer7>
    <GICP configFilename="hop3dGICP.xml"></GICP>
  </PartSelector>
